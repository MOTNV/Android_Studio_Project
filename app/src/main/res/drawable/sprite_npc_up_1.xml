<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size android:width="64dp" android:height="64dp" />
            <solid android:color="#EF5350" />
            <stroke android:width="2dp" android:color="#C62828" />
        </shape>
    </item>
    <item android:top="10dp" android:bottom="22dp" android:left="10dp" android:right="10dp">
        <shape android:shape="rectangle">
            <size android:height="8dp" />
            <solid android:color="#C62828" />
            <corners android:radius="6dp" />
        </shape>
    </item>
</layer-list>

