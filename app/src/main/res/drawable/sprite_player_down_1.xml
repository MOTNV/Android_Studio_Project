<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size android:width="64dp" android:height="64dp" />
            <solid android:color="#42A5F5" />
            <stroke android:width="2dp" android:color="#1E88E5" />
        </shape>
    </item>
    <item android:top="18dp" android:bottom="6dp" android:left="10dp" android:right="10dp">
        <shape android:shape="rectangle">
            <size android:height="8dp" />
            <solid android:color="#1E88E5" />
            <corners android:radius="6dp" />
        </shape>
    </item>
</layer-list>

