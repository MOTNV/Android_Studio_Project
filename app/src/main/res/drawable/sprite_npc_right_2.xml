<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size android:width="64dp" android:height="64dp" />
            <solid android:color="#EF5350" />
            <stroke android:width="2dp" android:color="#C62828" />
        </shape>
    </item>
    <item android:right="10dp" android:left="24dp" android:top="22dp" android:bottom="22dp">
        <shape android:shape="rectangle">
            <solid android:color="#B71C1C" />
            <corners android:radius="6dp" />
        </shape>
    </item>
</layer-list>

